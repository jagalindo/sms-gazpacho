@Article{Thum2014,
  Title                    = {A classification and survey of analysis strategies for software product lines},
  Author                   = {Th{\"u}m, T.a and Apel, S.b and K{\"a}stner, C.c and Schaefer, I.d and Saake, G.a},
  Journal                  = {ACM Computing Surveys},
