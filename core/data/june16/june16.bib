@InProceedings{KaratasOuztuezuenDoru2010a,
  Title                    = {Mapping extended feature models to constraint logic programming over finite domains},
  Author                   = {Karata{\c s}, A.S. and O{\v g}uzt{\"u}z{\"u}n, H. and Do{\v g}ru, A.},
