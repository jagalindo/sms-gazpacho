@Article{Hubaux2013641,
  Title                    = {Supporting multiple perspectives in feature-based configuration},
  Author                   = {Hubaux, A.a and Heymans, P.a b and Schobbens, P.-Y.a and Deridder, D.c and Abbasi, E.K.a},
