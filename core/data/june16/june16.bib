@Article{Del-Rio-Ortega2013470,
  Title                    = {On the definition and design-time analysis of process performance indicators},
  Author                   = {Del-R{\'\i}o-Ortega, A. and Resinas, M. and Cabanillas, C. and Ruiz-Cort{\'e}s, A.},
