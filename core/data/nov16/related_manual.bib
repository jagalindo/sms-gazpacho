@InProceedings{Acher2014,
  Title                    = {Customization and 3D printing: A challenging playground for software product lines},
  Author                   = {Acher, Mathieu and Baudry, Benoit and Barais, Olivier and J{\'{e}}z{\'{e}}quel, Jean Marc},
  Booktitle                = {SPLC},
